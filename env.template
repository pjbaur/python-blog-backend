MONGO_URI = mongodb://<username>:<password>@<host>:<port>/
SECRET_KEY = <secret_key>
ALGORITHM = HS256
ACCESS_TOKEN_EXPIRE_MINUTES = 30
INITIAL_ADMIN_EMAIL = <admin_email>
INITIAL_ADMIN_PASSWORD = <admin_password>
S3_BUCKET_NAME = mybucket
S3_REGION = us-east-1
S3_ACCESS_KEY_ID = YOUR_ACCESS_KEY_ID
S3_SECRET_ACCESS_KEY = YOUR_SECRET_ACCESS_KEY
S3_ENDPOINT_URL = https://s3.amazonaws.com
S3_USE_SSL = true
S3_VERIFY_SSL = true
S3_MAX_CONTENT_LENGTH = 10485760  # 10 MB
S3_ALLOWED_FILE_TYPES = image/jpeg, image/png, application/pdf
S3_MAX_FILES = 5
S3_MAX_FILE_NAME_LENGTH = 255
S3_MAX_FILE_PATH_LENGTH = 255
S3_MAX_FILE_SIZE_MB = 10

# Logging Configuration
LOG_LEVEL = INFO  # Available levels: TRACE, DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FORMAT = %(asctime)s - %(name)s - %(levelname)s - %(message)s
LOG_DATE_FORMAT = %Y-%m-%d %H:%M:%S
LOG_FILE = app.log
LOG_MAX_SIZE_MB = 10
LOG_BACKUP_COUNT = 3
CONSOLE_LOGGING = true